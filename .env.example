# Archivo de ejemplo .env para Plataforma Reflex

# Configuración de SQL Server
SQL_SERVER=servidor-reflex.database.windows.net
SQL_DATABASE=Database_Reflex
SQL_USER=usuario_sql
SQL_PASSWORD=contraseña_segura

# Puerto del backend
PORT=3000

# Clave secreta para JWT (si usas autenticación)
JWT_SECRET=una_clave_muy_segura

# Azure Blob Storage
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=refleximagenes;AccountKey=tu_clave;EndpointSuffix=core.windows.net
